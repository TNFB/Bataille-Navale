<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".flotte_menu">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <Button
            android:id="@+id/flotte1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/flotte1" />

        <Button
            android:id="@+id/flotte2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/flotte2" />

        <Button
            android:id="@+id/flotte3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/flotte3" />

        <Button
            android:id="@+id/flotte4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/flotte4" />
        <com.CoMaTo.bataillenavale.GameView
            android:id="@+id/GameView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="400dp"
            />
    </LinearLayout>



</FrameLayout>